{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "nodemon ./src/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "del /q build",
    "build-babel": "babel ./src -d ./build/src",
    "copy": "cpx \"*.{json,env}\" \"build/src\"",
    "install-package": "cd ./build/src && npm install",
    "build": "npm run clean && npm run build-babel && npm run copy",
    "production": "npm run build && node ./build/src/app.js",
    "dev": "nodemon --exec ./node_modules/.bin/babel-node ./src/app.js",
    "knex-clear": "cd src/db/ && knex migrate:rollback --all && knex migrate:latest && knex seed:run"
  },
  "author": "",
  "license": "ISC",
